{
    "[latex]": {
        "editor.tabSize": 2
    },
    "[latex-expl3]": {
        "editor.tabSize": 2
    },
    "[lua]": {
        "editor.tabSize": 2
    },
    "busted-test-explorer.args": [
        "-lua=texlua",
        "-lpath=./citeproc/?.lua"
    ],
    "busted-test-explorer.cwd": "${workspaceFolder}",
    "busted-test-explorer.testfilepattern": "tests/*test.lua",
    "files.insertFinalNewline": true,
    "files.trimFinalNewlines": true,
    "files.trimTrailingWhitespace": true,
    "files.associations": {
        ".busted": "lua",
        "*.aux": "latex",
        "*.bbl": "latex",
        "*.lvt": "latex",
        "*.rockspec": "lua",
        "*.sty": "latex-expl3",
        "*.tlg": "latex-expl3",
        "*_*.txt": "csl-test",
        "DEPENDS.txt": "shellscript",
        "citeproc-test-skip.txt": "shellscript"
    },
    "latex-workshop.intellisense.update.aggressive.enabled": true,
    "Lua.format.defaultConfig": {
        "indent_size": "2",
        "indent_style": "space",
        "space_around_table_field_list": "false",
        "space_before_closure_open_parenthesis": "true"
    },
    "Lua.runtime.version": "Lua 5.3"
}
